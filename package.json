{
  "name": "skewered",
  "version": "1.0.0",
  "description": "The simplest slugify module in the universe.",
  "homepage": "https://github.com/busterc/skewered",
  "author": {
    "name": "Buster Collings",
    "email": "busterc@gmail.com",
    "url": "https://about.me/buster"
  },
  "files": [
    "lib"
  ],
  "main": "lib/index.js",
  "keywords": [
    "slugify",
    "slug",
    "urlify",
    "dashify",
    "dashcase",
    "dash-case",
    "kebabify",
    "kebabcase",
    "kebab-case",
    "hyphenate",
    "hyphencase",
    "hyphen-case",
    "skewer"
  ],
  "devDependencies": {
    "coveralls": "^3.0.2",
    "nsp": "^3.2.1",
    "eslint": "^5.4.0",
    "prettier": "^1.7.0",
    "husky": "^0.14.3",
    "lint-staged": "^7.2.2",
    "eslint-config-prettier": "^3.0.1",
    "eslint-plugin-prettier": "^2.2.0",
    "eslint-config-xo": "^0.24.2",
    "jest": "^23.5.0",
    "jest-cli": "^23.5.0"
  },
  "scripts": {
    "prepublish": "nsp check",
    "pretest": "eslint .",
    "precommit": "lint-staged",
    "test": "jest --coverage"
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ],
    "*.json": [
      "prettier --write",
      "git add"
    ]
  },
  "eslintConfig": {
    "extends": [
      "xo",
      "prettier"
    ],
    "env": {
      "jest": true,
      "node": true
    },
    "rules": {
      "prettier/prettier": [
        "error",
        {
          "singleQuote": true,
          "printWidth": 90
        }
      ]
    },
    "plugins": [
      "prettier"
    ]
  },
  "repository": "busterc/skewered",
  "jest": {
    "testEnvironment": "node"
  },
  "license": "ISC"
}
